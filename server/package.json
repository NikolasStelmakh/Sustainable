{
  "name": "sustainable-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "build": "npx tsc",
    "migrate:create": "prisma migrate dev --create-only",
    "migrate:reset": "concurrently \"prisma migrate reset --force\" \"npm run seed\"",
    "migrate:dev": "prisma migrate dev",
    "migrate:prod": "prisma migrate deploy",
    "seed": "ts-node --esm prisma/seed.ts",
    "server:prod": "concurrently \"npm run migrate:prod\" \"node --experimental-specifier-resolution=node build/index.js\"",
    "server:dev": "concurrently \"npx tsc --watch\" \"nodemon -q --experimental-specifier-resolution=node build/index.js\"",
    "eslint:fix": "eslint --fix --ignore-path .eslintignore --ext .js,.ts .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.3.0",
    "@prisma/client": "^4.8.1",
    "body-parser": "^1.20.1",
    "bunyan": "^1.8.15",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-depth-limit": "^1.1.0"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.8",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/graphql-depth-limit": "^1.1.3",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.2",
    "prisma": "^4.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
